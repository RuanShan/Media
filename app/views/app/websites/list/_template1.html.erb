<div class="body">
  <div class="box box-list">
    <% website_menu.sub_menus.each do |child| %>
        <a href="<%= menu_url child %>" class="bg-arrow table">
          <span class="fl td">
            <b><%= image_tag child.pic.thumb if child.pic? %></b>
          </span>
          <span class="fr td">
            <b><%= child.name %></b>
            <!--<i><%#= date_format_full_l(child.created_at)  %></i>-->
          </span>
        </a>
    <% end %>
  </div>
</div>
