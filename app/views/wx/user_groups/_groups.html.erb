<li class="active" id="group--1"><div><span class="J-text group" data-group='-1'>全部用户</span><span class="num"><%= @users.length %></span></div></li>

<%- groups.each do |group| %>
<li id="group-<%= group.groupid %>"><div><span class="J-group-name group"  data-group='<%= group.groupid %>' ><%= group.name %></span><span class="num"><%= group.count  %></span></div>
<i class="fa fa-edit vipcard-edit J-editGroup" data-id="<%= group.groupid %>"></i>
<i class="fa fa-times vipcard-del J-deleteGroup" data-id="<%= group.groupid %>"></i>
</li>
<% end %>
