<% current_name = @hospital_job_title.name %>
<% if current_site.hospital.hospital_job_titles.where(name: current_name).count > 0 %>
	showTip('warning', '已存在相同的职称，无法新增');
<% elsif @hospital_job_title.save %>
  $("#title_lable").append("<%= escape_javascript(render('job_title', hospital_job_title: @hospital_job_title)) %>");
  $("#hidden_title_div").append('<input id="hospital_job_title_ids" name="hospital_doctor[hospital_job_title_ids][]" type="checkbox" value="<%= @hospital_job_title.id %>" checked>');
<% else %>
  showTip('warning', '职称保存失败');
<% end %>