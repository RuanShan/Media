<!doctype html>
<html xmlns="http://www.w3.org/1999/html">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, width=device-width">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta content="black" name="apple-mobile-web-app-status-bar-style">
  <meta name="format-detection" content="telephone=no">
  <meta content="telephone=no" name="format-detection">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <title>语音页面</title>
  <link type="text/css" rel="stylesheet" href="/stylesheets/vweisiteV01.css"/>
  <link type="text/css" rel="stylesheet" href="/stylesheets/vweisiteV01/index/index1.css"/>
  <link type="text/css" rel="stylesheet" href="/stylesheets/font-awesome.css"/>
</head>
<body class="body-navLine-bottom">
<div class="html">
  <div class="stage" id="stage">
    <section id="sec-index">
      <!--<header></header>-->
      <div class="body">
        <header></header>
        <div class="audio">
          <audio autoplay controls="true" src="<%= @audio.try(:audio_absolute_path) %>"></audio>
        </div>
      </div><!--.body end -->
    </section><!--main section end -->
    <section class="mod-navLine navLine-2">
      <ul class="navLine-ul">
        <li>
          <a class="active dev-prev" onclick="history.back();"><i class="fa fa-reply"></i><small>返回</small></a>
        </li>
        <li>
          <a class="dev-refresh" onclick="location.reload();"><i class="fa fa-refresh"></i><small>刷新</small></a>
        </li>
      </ul>
    </section>

  </div><!--.stage end-->
</div><!--.html end-->
<%= render 'application/app_footer' %>
</body>
</html>
