<% title @wx_plot.try(:owner) %>
<div class="body">
  <div class="index-list">
    <% @owners.each do |f| %>
        <%= link_to mobile_wbbs_topics_url(site_id: @site.id, aid: f.activity.id), class: 'box' do %>
            <i><%= image_tag f.logo_url %></i>
            <span><%= f.name %></span>
        <% end %>
    <% end %>
  </div>
</div>