<li class="active" id="group--1"><div><span class="J-text group" data-group='-1'>全部用户</span><span class="num"><%= all_vip_users_count %></span></div></li>
<li id="group--2"><div><span class="J-group-name  group" data-group='-2'>未分组</span><span class="num"><%= no_group_vip_users_count %></span></div></li>
<%- vip_groups.each do |group| %>
<li id="group-<%= group.id %>"><div><span class="J-group-name group"  data-group='<%= group.id %>' ><%= group.name %></span><span class="num"><%= group.vip_users_count  %></span></div>
<i class="fa fa-edit vipcard-edit J-editGroup" data-id="<%= group.id %>"></i>
<i class="fa fa-times vipcard-del J-deleteGroup" data-id="<%= group.id %>"></i>
</li>
<% end %>
