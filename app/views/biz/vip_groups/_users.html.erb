<%- vip_users.each do |user|%>
  <li  data-id='<%= user.id %>'>
    <div class="vipcard-info">
      <h1><%= user.name %></h1>
      <% if current_user.vip_card.labeled_custom_field_ids.present? %>
        <p><%= raw vip_labeled_values(current_user.vip_card, user) %></p>
      <% end %>
      <p><%= user.vip_grade_name %></p>
      <p><span><%= user.vip_group.name rescue '未分组' %></span></p>
    </div>
    <i class="vipcard-checked fa fa-check"></i>
    <span><a class="vip-detail left" data-toggle="modals" href="javascript:;" data-title="会员资料" data-height="1000" data-iframe="<%= edit_vip_user_path(user, view: 1) %>">会员详情</a></span>
    <%- if user.vip_group %>
      <a class="vipcard-list-del" href="javascript:;" data-id='<%= user.id %>'>删除</a>
    <%- end %>
  </li>
<%- end %>