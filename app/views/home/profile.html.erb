<% content_for :main_content do %>
  <div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
      <ul class="breadcrumb">
        <%= render 'partials/home' %>
        <li><%= link_to '系统设置', profile_path %></li>
        <li><%= link_to '我的账号', 'javascript:;' %></li>
      </ul>
      <%#= render '/layouts/qrcode' %>
    </div>

    <div class="page-content">
      <div class="row">
        <div class="col-sm-12">
          <table id="" class="table table-striped table-bordered table-hover dataTable ">
            <tbody>
            <tr>
              <th colspan="2">我的账号</th>
            </tr>
            <tr>
              <td>登录账号：</td>
              <td><%= current_user.nickname %></td>
            </tr>
            <tr>
              <td>用户ID：</td>
              <td><%= current_user.id %></td>
            </tr>
            <tr>
              <td>手机号码：</td>
              <td><%= current_user.mobile %></td>
            </tr>
            <tr>
              <td>电子邮件：</td>
              <td><%= current_user.email %></td>
            </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
<% end %>
